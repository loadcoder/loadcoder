<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<!-- result test -->
	<appender name="CONSOLEAPPENDER"
		class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<Pattern>
				%m%n
			</Pattern>
		</layout>
	</appender>

	<root level="debug">
		<appender-ref ref="CONSOLEAPPENDER" />
	</root>

</configuration>